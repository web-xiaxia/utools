{
    "main": "index.html",
    "preload": "preload.js",
    "logo": "logo.png",
    "platform": [
        "win32",
        "darwin",
        "linux"
    ],
    "development": {
        "main": "http://localhost:3000"
    },
    "pluginSetting": {
        "single": false,
        "height": 0
    },
    "features": [
        {
            "code": "Json编辑器",
            "explain": "json编辑器",
            "platform": [
                "win32",
                "darwin",
                "linux"
            ],
            "cmds": [
                "Json",
                {
                    "type": "regex",
                    "label": "JSON",
                    "match": "/^(?={)[\\s\\S]*(?<=})[\\r]*[\\n]*|^(?=\\[)[\\s\\S]*(?<=\\])[\\r]*[\\n]*/i",
                    "minLength": 2
                }
            ]
        },
        {
            "code": "Json编辑器SUPER",
            "explain": "json编辑器SUPER",
            "platform": [
                "win32",
                "darwin",
                "linux"
            ],
            "cmds": [
                "Json-Super",
                {
                    "type": "regex",
                    "label": "JSON-SUPER",
                    "match": "/.*{.*?}.*/i",
                    "minLength": 2
                },
                {
                    "type": "regex",
                    "label": "JSON-SUPER",
                    "match": "/.*\\[.*?\\].*/i",
                    "minLength": 2
                }
            ]
        }
    ]
}
